<div class="gallery-sidebar">
  <ul class="list-unstyled">
  <% @nav_items.each do |key, val| %>
    <li class="<%= nav_class(val[:nav_class]) %>">
      <%= link_to val[:url] do %>
        <i class="<%= val[:icon_class] %>"></i>
        <span><%= val[:text] %></span>
      <% end %>
    </li>
  <% end %>
  </ul>
</div>