<div class="row">
      
  <div class="col-md-12">
    
    <ul class="nav nav-tabs">
    </ul>
    <div class="tab-content">
      <div class="tab-pane active">

        <div id="div_event_action_buttons">
          
          <div class="row">
            <div class="col-sm-6">
              <%= theme_button('Add a Event', 'plus', new_event_path(), classes: "pull-left", btn_type: "success") %>

              <%= theme_button('Refresh', 'refresh', events_path(), classes: "pull-left ml-10", btn_type: "white") %>

              <!-- Single button -->
              <div class="ml-10 btn-group">
                <button type="button" class="btn btn-white dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                  More Actions <span class="caret"></span>
                </button>

                <ul class="dropdown-menu">
                  <li><%= theme_button('Import Events', 'upload', import_data_link(nil, "Event"), classes: "pull-left ml-5 mb-5", btn_type: "default") %></li>
                  <li><%= theme_button('Import History', 'history', "#", classes: "pull-left ml-5 mb-5", btn_type: "default") %></li>
                  <li role="separator" class="divider"></li>
                  <li><%= theme_button('Export Users', 'download', "#", classes: "pull-left ml-5 mb-5", btn_type: "default") %></li>
                </ul>
              </div>
            </div>
            <div class="col-sm-6">
              <%= search_form_kuppayam(Event, events_path, text: @filters[:query]) %>
            </div>
          </div>

        </div>
        <%= clear_tag(10) %>

        <div id="div_event_index">
          <%= render :partial=>"demo/events/index" %>
        </div>
        <%= clear_tag(10) %>

      </div>
    </div>

  </div>

</div>

